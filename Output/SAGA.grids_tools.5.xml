<tool id="SAGA.grids_tools.5" name="Nearest Neighbour (3D)" version="1.0.0"><description>Nearest neighbour interpolation for 3-dimensional data points. Output will be a grid collection with evenly spaced Z-levels representing the 3rd dimension. </description><inputs><param name="POINTS" label="Points" help="Points" type="data" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="Z_FIELD" label="Z" help="Z" type="text" format="text/csv, text/csv" /><param name="Z_SCALE" label="Z Factor" help="Z Factor" type="float" format="text/csv, text/csv" /><param name="V_FIELD" label="Value" help="Value" type="text" format="text/csv, text/csv" /><param name="TARGET_DEFINITION" label="Target Grid System" help="Target Grid System" type="text" format="text/csv, text/csv" /><param name="TARGET_USER_SIZE" label="Cellsize" help="Cellsize" type="float" format="text/csv, text/csv" /><param name="TARGET_USER_XMIN" label="West" help="West" type="float" format="text/csv, text/csv" /><param name="TARGET_USER_XMAX" label="East" help="East" type="float" format="text/csv, text/csv" /><param name="TARGET_USER_YMIN" label="South" help="South" type="float" format="text/csv, text/csv" /><param name="TARGET_USER_YMAX" label="North" help="North" type="float" format="text/csv, text/csv" /><param name="TARGET_USER_COLS" label="Columns" help="Number of cells in East-West direction." type="integer" format="text/csv, text/csv" /><param name="TARGET_USER_ROWS" label="Rows" help="Number of cells in North-South direction." type="integer" format="text/csv, text/csv" /><param name="TARGET_USER_FITS" label="Fit" help="Fit" type="text" format="text/csv, text/csv" /><param name="TARGET_TEMPLATE" label="Target System" help="use this grid's system for output grids" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_USER_ZSIZE" label="Cellsize" help="Cellsize" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc