<tool id="SAGA.grid_analysis.0" name="Accumulated Cost" version="1.0.0"><description>Calculation of accumulated cost, either isotropic or anisotropic, if direction of maximum cost is specified. </description><inputs><param name="DEST_TYPE" label="Input Type of Destinations" help="Input Type of Destinations" type="text" /><param name="DEST_POINTS" label="Destinations" help="Destinations" type="data" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="DEST_GRID" label="Destinations" help="Destinations" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="COST" label="Local Cost" help="Local Cost" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="DIR_MAXCOST" label="Direction of Maximum Cost" help="Direction of Maximum Cost" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="DIR_UNIT" label="Units of Direction" help="Units of Direction" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="DIR_K" label="K Factor" help="effective friction = stated friction ^f , where f = cos(DifAngle)^k." type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="THRESHOLD" label="Threshold for different route" help="Threshold for different route" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /></inputs><outputs><data name="ACCUMULATED" label="Accumulated Cost" /><data name="ALLOCATION" label="Allocation" /></outputs></tool>