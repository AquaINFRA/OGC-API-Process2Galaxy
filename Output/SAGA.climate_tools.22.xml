<tool id="SAGA.climate_tools.22" name="PhenIps (Grids)" version="1.0.0"><description>A comprehensive phenology model of Ips typographus (L.) (Col., Scolytinae) as a tool for hazard rating of bark beetle infestation.</description><inputs><param name="ATmean" label="Mean Temperature" help="Degree Celsius" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="ATmax" label="Maximum Temperature" help="Degree Celsius" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="SIrel" label="Solar Irradiation" help="Wh/m^2" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="LATITUDE" label="Latitude" help="Latitude" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="DToptimum" label="Developmental Optimum Temperature" help="Degree Celsius" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="DTminimum" label="Developmental Minimum Temperature" help="Degree Celsius" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="FAminimum" label="Minimum Temperature for Flight Activity" help="Degree Celsius" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="DDminimum" label="Minimum Thermal Sum for Infestation" help="Degree Days" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="DDtotal" label="Thermal Sum for Total Development" help="Degree Days" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="YD_Begin_MIN" label="Begin of Parental Development (min value)" help="Begin of Parental Development" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="YD_End_Onset_MIN" label="End of Breeding (min value)" help="End of Breeding" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="YD_End_MIN" label="End of Development (min value)" help="End of Development" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /></inputs><outputs><data name="ONSET" label="Onset Day of Infestation" /><data name="NUMBER" label="Potential Number of Generations" /><data name="ONSET_FILIAL_1" label="Onset Day of 1st Filial Generation" /><data name="ONSET_SISTER_1" label="Onset Day of 1st Sister Generation" /><data name="ONSET_FILIAL_2" label="Onset Day of 2nd Filial Generation" /><data name="ONSET_SISTER_2" label="Onset Day of 2nd Sister Generation" /><data name="ONSET_FILIAL_3" label="Onset Day of 3rd Filial Generation" /><data name="ONSET_SISTER_3" label="Onset Day of 3rd Sister Generation" /><data name="STATE_FILIAL_1" label="State of 1st Filial Generation" /><data name="STATE_SISTER_1" label="State of 1st Sister Generation" /><data name="STATE_FILIAL_2" label="State of 2nd Filial Generation" /><data name="STATE_SISTER_2" label="State of 2nd Sister Generation" /><data name="STATE_FILIAL_3" label="State of 3rd Filial Generation" /><data name="STATE_SISTER_3" label="State of 3rd Sister Generation" /></outputs></tool>