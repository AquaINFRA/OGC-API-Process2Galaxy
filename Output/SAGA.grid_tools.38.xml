<tool id="SAGA.grid_tools.38" name="Mosaicking (Grid Collections)" version="1.0.0"><description>Merges multiple grid collections into one single grid collection. Input grid collections have to share the same number of grid levels. Attributes and other general properties will be inherited from the first grid collection in input list. </description><inputs><param name="GRIDS" label="Grid Collections" help="Grid Collections" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="NAME" label="Name" help="Name" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TYPE" label="Data Storage Type" help="Data Storage Type" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="RESAMPLING" label="Resampling" help="Resampling" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="OVERLAP" label="Overlapping Areas" help="Overlapping Areas" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="BLEND_DIST" label="Blending Distance" help="blending distance given in map units" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="BLEND_BND" label="Blending Boundary" help="blending boundary for distance calculation" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="MATCH" label="Match" help="Match" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_DEFINITION" label="Target Grid System" help="Target Grid System" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_USER_SIZE" label="Cellsize" help="Cellsize" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_USER_XMIN" label="West" help="West" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_USER_XMAX" label="East" help="East" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_USER_YMIN" label="South" help="South" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_USER_YMAX" label="North" help="North" type="float" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_USER_COLS" label="Columns" help="Number of cells in East-West direction." type="integer" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_USER_ROWS" label="Rows" help="Number of cells in North-South direction." type="integer" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_USER_FITS" label="Fit" help="Fit" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /><param name="TARGET_TEMPLATE" label="Target System" help="use this grid's system for output grids" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /></inputs><outputs><data name="MOSAIC" label="Mosaic" /></outputs></tool>