<tool id="SAGA.grid_gridding.8" name="Grid Cell Area Covered by Polygons" version="1.0.0"><description>This tool calculates for each grid cell of the selected grid system the area that is covered by the input polygons layer. </description><inputs><param name="POLYGONS" label="Polygons" help="Polygons" type="data" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="METHOD" label="Method" help="Choose cell wise, if you have not many polygons. Polygon wise will show much better performance, if you have many (small) polygons." type="text" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="OUTPUT" label="Output" help="Output" type="text" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="SELECTION" label="Only Selected Polygons" help="Only Selected Polygons" type="boolean" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_DEFINITION" label="Target Grid System" help="Target Grid System" type="text" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_USER_SIZE" label="Cellsize" help="Cellsize" type="float" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_USER_XMIN" label="West" help="West" type="float" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_USER_XMAX" label="East" help="East" type="float" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_USER_YMIN" label="South" help="South" type="float" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_USER_YMAX" label="North" help="North" type="float" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_USER_COLS" label="Columns" help="Number of cells in East-West direction." type="integer" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_USER_ROWS" label="Rows" help="Number of cells in North-South direction." type="integer" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_USER_FITS" label="Fit" help="Fit" type="text" format="text/xml, application/vnd.google-earth.kml+xml" /><param name="TARGET_TEMPLATE" label="Target System" help="use this grid's system for output grids" type="text" format="image/tiff, application/x-ogc-envi, application/x-ogc-aaigrid, image/png" /></inputs><outputs><data name="AREA" label="Area of Coverage" /></outputs></tool>